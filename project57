#include <iostream>
using namespace std;
int main()
{
 int num;
 cout<<"Enter the number of credits:";
 cin>>num;
 if (num<=15)
  {
   cout<<"Lower Freshman\n";
  }
 else if (15<num && num<28)
  {
   cout<<"Upper Freshman\n";
  }
 else if (28<num && num<45)
  {
   cout<<"Lower Sophomore\n";
  }
 else if (45<num && num<61)
  {
   cout<<"Upper Sophomore\n";
  }
 else if (61<num && num<75)
  {
   cout<<"Lower Junior\n";
  }
 else if (75<num && num<94)
  {
   cout<<"Upper Junior\n";
  }
 else if (94<num && num<108)
  {
   cout<<"Lower Senior\n";
  }
 else{
   cout<<"Upper Senior\n";
  }
 return 0;
 }
